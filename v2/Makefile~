SRC	=	string/my_putchar.c		\
		string/my_putstr.c		\
		string/my_putnbr.c		\
		string/my_strcmp.c		\
		string/my_strncmp.c		\
		string/my_strlen.c		\
		string/my_getnbr.c

OBJ	=	$(SRC:.c=.o)

NAME	=	librat.a

INCLUDE = -I./include

CFLAGS += $(INCLUDE) -g -W -Wall -Wextra -Werror

all:	$(NAME)

$(NAME):	$(OBJ)
	@ar rc $(NAME) $(OBJ)
	@make clean

clean:
	@rm -f $(OBJ)

fclean:	clean
	@rm -f $(NAME)

re:	fclean all
